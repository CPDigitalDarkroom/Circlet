			<key>label</key> 		<string>Size</string>
            <key>cellClass</key>    <string>CRSegmentTableCell</string>
			<key>validTitles</key>  <array>
				<string>Small</string>
				<string>Medium</string>
				<string>Large</string>
			</array>
			<key>validValues</key> <array>
				<real>3.0</real>
				<real>4.0</real>
				<real>5.0</real>
			</array>
		</dict>

